{
  "name": "kebacorp/vaultsecret",
  "description": "The extension allows to load the Vault secrets from json files and get them",
  "keywords": [
    "vault"
  ],
  "homepage": "https://github.com/KebaCorp/VaultSecret",
  "type": "extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Abek Narynov",
      "homepage": "https://www.facebook.com/abeknarynov"
    }
  ],
  "support": {
    "source": "https://github.com/KebaCorp/VaultSecret"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.3",
    "ext-json": "*",
    "ext-curl": "*",
    "psr/simple-cache": "*"
  },
  "require-dev": {},
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "KebaCorp\\VaultSecret\\": "src",
      "app\\": ""
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
